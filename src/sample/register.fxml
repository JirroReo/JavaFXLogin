<AnchorPane prefHeight="437.0" prefWidth="0.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.RegisterController">
   <children>
      <ImageView fx:id="registerImageView" fitHeight="492.0" fitWidth="313.0" layoutX="-14.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../resources/281.jpg" />
         </image>
      </ImageView>
      <JFXButton fx:id="cancelButton" contentDisplay="TEXT_ONLY" layoutX="14.0" layoutY="14.0" minWidth="27.0" onAction="#CancelButtonOnAction" prefHeight="32.0" prefWidth="27.0" style="-fx-background-color: #ff0000;" text="X" textFill="WHITE">
         <font>
            <Font size="13.0" />
         </font>
      </JFXButton>
      <JFXTextField fx:id="fnameTextField" labelFloat="true" layoutX="58.0" layoutY="162.0" prefHeight="26.0" prefWidth="97.0" promptText="First Name" />
      <JFXTextField fx:id="lnameTextField" labelFloat="true" layoutX="167.0" layoutY="162.0" prefHeight="26.0" prefWidth="97.0" promptText="Last Name" />
      <JFXTextField fx:id="unameTextField" labelFloat="true" layoutX="62.0" layoutY="213.0" prefHeight="26.0" prefWidth="177.0" promptText="Username" />
      <JFXPasswordField fx:id="pwTextField" labelFloat="true" layoutX="62.0" layoutY="262.0" prefHeight="26.0" prefWidth="177.0" promptText="Password" />
      <JFXPasswordField fx:id="confirmPwTextField" labelFloat="true" layoutX="62.0" layoutY="316.0" prefHeight="26.0" prefWidth="177.0" promptText="Retype Password" />
      <JFXButton fx:id="registerButton" contentDisplay="TEXT_ONLY" layoutX="97.0" layoutY="389.0" minWidth="26.0" onAction="#registerButtonOnAction" prefHeight="32.0" prefWidth="86.0" style="-fx-background-color: #4059a9;" text="Register" textFill="WHITE">
         <font>
            <Font size="13.0" />
         </font>
      </JFXButton>
      <Label fx:id="errorMessage" layoutX="71.0" layoutY="357.0" prefHeight="18.0" prefWidth="158.0" textFill="RED" />
      <Label layoutX="113.0" layoutY="105.0" prefHeight="49.0" prefWidth="139.0" text="REGISTER" textFill="#4059a9">
         <font>
            <Font name="Montserrat ExtraBold" size="25.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
